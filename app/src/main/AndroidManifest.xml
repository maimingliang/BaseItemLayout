<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.maiml.baseui">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NormalActivity"></activity>
        <activity android:name=".TxtActivity"></activity>
        <activity android:name=".ButtonActivity"></activity>
        <activity android:name=".ArrowActivity"></activity>
        <activity android:name=".RedTextActivity"></activity>
        <activity android:name=".MixActivity"></activity>
    </application>

</manifest>